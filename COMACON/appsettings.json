{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": {
        "Default": "Warning"
      },
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": {
            "path": "logs\\logging-.txt",
            "rollingInterval": "Day"
          }
        }
      ]
    },
    "AllowedHosts": "*",
    "LooseFilesLocation": ""
  }
