<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>COMACON</title>
        @*Stylesheets*@
        <link rel="stylesheet" href="~/css/style_webserver.css">
        @*Scripts*@
        <script src="~/js/WebServer_Core.js"></script>
        <script src="~/js/GeneralWebApplication.js"></script>
        <script src="~/js/ApplicationServer.js"></script>
        <script src="~/js/ReportingViewerServer.js"></script>
        <script src="~/js/GatewayCachingServer.js"></script>
        <script src="~/js/AgendaOnlineServer.js"></script>
        <script src="~/js/ElectronicPlanReview.js"></script>
        <script src="~/js/HealthcareFormManager.js"></script>
        <script src="~/js/PublicAccessViewerNextGen.js"></script>
        <script src="~/js/PublicAccessViewerLegacy.js"></script>
        <script src="~/js/OnBasePatientWindow.js"></script>
        <script src="~/js/LocalConfigFile_Core.js"></script>
    </head>
    <body id="myBody">
        <template id="DiagnosticsSettingsRouteProfiles">
            <div class="setting-element">
                <input type="checkbox" name="" id="input" class="Profiles" onclick="">
                <label for="label"></label>
            </div>
        </template>
        <!-- Choose Web Application Modal Window -->
        <div id="chooseWebApplicationModal" class="chooseWebApplicationModal">
            <!-- Modal content -->
            <div class="chooseWebApplicationModal-content">
                <!-- Modal header -->
                <div id="ChooseApplicationTitleBar" class="ChooseApplicationTitleBar">
                    <div>
                        <h2>Choose Application</h2>
                    </div>
                    <div>
                        <span class="close" onclick="closeModalX()">&times;</span>
                    </div>
                </div>
                <!-- Loading Web Applications Progress Bar -->
                <div id="Loading-Web-Applications-Progress-Section" class="loadingWebApplicationsProgressSection">
                    <label class="loadingWebApplicationsProgressLabel" for="Loading-Web-Applications-Progress-Bar">Getting Web Applications...</label>
                    <progress class="loadingWebApplicationsProgressBar" id="Loading-Web-Applications-Progress-Bar"></progress>
                </div>
                <div id="chooseWebApplicationModal-Table-Buttons-Container" class="chooseWebApplicationModalTableButtonsContainer">
                    <!-- Modal body -->
                    <div class="chooseWebApplicationModal-Table-Wrapper">
                        <table id="webApplicationTable">
                            <thead>
                                <tr>
                                    <th number=1 id="table-head-Name" onclick="sortTable(0,'webApplicationTable')">Name</th>
                                    <th number=2 id="table-head-Type" onclick="sortTable(1,'webApplicationTable')">Type</th>
                                    <th number=3 id="table-head-Version" onclick="sortTable(2,'webApplicationTable')">Version</th>
                                    <th number=4 id="table-head-Site" onclick="sortTable(3,'webApplicationTable')">Site</th>
                                    <th number=5 id="table-head-Location" onclick="sortTable(4,'webApplicationTable')">Location</th>
                                    <th number=6 id="table-head-Bitness" onclick="sortTable(5,'webApplicationTable')">Bitness</th>
                                </tr>
                            </thead>
                            <!-- <tbody></tbody> -->
                        </table>
                    </div>
                    <div class="chooseWebApplication-Buttons">
                        <button id="Refresh-Button" class="chooseWebApplication-RefreshButton" onclick="loadWebApplications()">Refresh</button>
                        <button id="Open-Button" class="chooseWebApplication-OpenButton" onclick="okButtonClicked()">Open</button>
                        <button id="Cancel-Button-ChooseWebApplication" class="chooseWebApplication-CancelButton" onclick="closeModalX()">Cancel</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- Copy Web Application Modal -->
        <div id="CopyWebApplicationModal" class="CopyWebApplicationModal">
            <!-- Window Container -->
            <div id="CopyWebApplicationModal-Container" class="CopyWebApplicationModal-Container">
                <!-- Title Bar Container for styling -->
                <div class="CopyWebApplicationModal-TitleBar-Container-Styling">
                    <!-- Actual Title Bar Container -->
                    <div class="CopyWebApplicationModal-TitleBar-Container">
                        <h2>Copy Web Application</h2>
                        <span class="close" onclick="closeCopyWebApplicationModal()">&times;</span>
                    </div>
                </div>
                <!-- Body Section -->
                <div class="CopyWebApplicationModal-BodySection">
                    <!-- Information Section -->
                    <div class="CopyWebApplicationModal-Body-InformationSection">
                        <p>All settings and Application folder location will be copied from the existing application.<br>(<span class="underline">NOTE: Application Name/Pool Name must be unique.</span>)</p>
                    </div>
                    <!-- Field Section -->
                    <div class="CopyWebApplicationModal-BodySection-FieldSection">
                        <!-- Field Container -->
                        <div class="CopyWebApplicationModal-Body-FieldContainer">
                            <!-- Label -->
                            <div class="CopyWebApplicationModal-Body-FieldContainer-Label">
                                <label for="CopyWebApplicationModal-NewApplicationName">New Application Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">This is the help text.</span>
                            </div>
                            <!-- Field -->
                            <div class="CopyWebApplicationModal-Body-FieldContainer-Field">
                                <input id="CopyWebApplicationModal-NewApplicationName" type="text" onfocusout="validateNoDuplicateApplicationName(this)" oninput="resetCopyApplicationFieldContainerStatusSvg('NewApplicationName-ResultContainer')">
                                <div id="NewApplicationName-ResultContainer">
                                </div>
                            </div>
                        </div>
                        <!-- Field Container -->
                        <div class="CopyWebApplicationModal-Body-FieldContainer">
                            <!-- Label -->
                            <div class="CopyWebApplicationModal-Body-FieldContainer-Label">
                                <label for="CopyWebApplicationModal-NewApplicationPoolName">New Application Pool Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">This is the help text.</span>
                            </div>
                            <!-- Field -->
                            <div class="CopyWebApplicationModal-Body-FieldContainer-Field">
                                <input type="text" id="CopyWebApplicationModal-NewApplicationPoolName" onfocusout="validateNoDuplicateApplicationPoolName(this)" oninput="resetCopyApplicationFieldContainerStatusSvg('NewApplicationPoolName-ResultContainer')">
                                <div id="NewApplicationPoolName-ResultContainer">
                                </div>
                            </div>
                        </div>
                        <!-- Field Container -->
                        <div id="CopyWebApplicationModal-ApplicationServerURLField" class="CopyWebApplicationModal-Body-FieldContainer">
                            <!-- Label -->
                            <div class="CopyWebApplicationModal-Body-FieldContainer-Label">
                                <label for="CopyWebApplicationModal-ApplicationServerUrl">Application Server URL: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">This is the help text.</span>
                            </div>
                            <!-- Field -->
                            <div class="CopyWebApplicationModal-Body-FieldContainer-Field">
                                <input type="url" id="CopyWebApplicationModal-ApplicationServerUrl" onfocusout="validateApplicationServerURL(this)" oninput="resetCopyApplicationFieldContainerStatusSvg('ApplicationServerUrl-ResultContainer')">
                                <div id="ApplicationServerUrl-ResultContainer">
                                </div>
                            </div>
                        </div>
                        <!-- Action Buttons -->
                        <div class="CopyWebApplicationModal-Body-ActionButtons">
                            <div class="CopyWebApplicationModal-Body-ActionButtons-ButtonsContainer">
                                <button id="Copy-Button" onclick="copyWebApplication()">Copy</button>
                                <button id="Cancel-Button-CopyWebApplication" onclick="closeCopyWebApplicationModal()">Cancel</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Successful save Modal -->
        <div class="modal-overlay" id="modalOverlay">
            <div class="modal-content">
                <svg id="successfulSave" width="120" height="120" viewBox="0 0 120 120" fill="none" xmlns="http://www.w3.org/2000/svg" style="display: none">
                    <circle cx="60" cy="60" r="52.5" fill="#4CAF50" />
                    <path d="M32.5 65L55 90L100 45" stroke="white" stroke-width="12" stroke-linecap="round" stroke-linejoin="round" />
                </svg>
                <svg id="erroredSave" width="120" height="120" viewBox="0 0 120 120" xmlns="http://www.w3.org/2000/svg" style="display: none">
                    <!-- Outer white circle -->
                    <circle cx="60" cy="60" r="52.5" fill="white" stroke="black" stroke-width="2" />
                    <!-- Red background circle -->
                    <circle cx="60" cy="60" r="45" fill="red" />
                    <!-- White railroad crossing X -->
                    <path d="M30 30 L90 90 M30 90 L90 30" stroke="white" stroke-width="12" stroke-linecap="round" stroke-linejoin="round" />
                </svg>
            </div>
        </div>
        <!-- Error Load Modal -->
        <div class="errorLoadModal" id="errorLoadModal">
            <div id="errorLoadModal-content" class="errorLoadModal-content">
                <div>
                    <p>The following error(s) happened when loading your Web Application:</p>
                    <ul id="errorLoadModal-ErrorList">
                        
                    </ul>
                </div>
                <div style="display:flex;">
                    <button onclick="closeErrorLoadModal()" style="margin:auto;">OK</button>
                </div>
            </div>
        </div>
        <!-- Connection String Test Modal -->
        <div class="connectionStringTestModal" id="connectionStringTestModal">
            <div id="connectionStringTestModal-content" class="connectionStringTestModal-content" style="">
                @* <div>
                    <p style="font:large;font-weight:bold;">Testing your connection string.</p>
                </div> *@
                <label style="font-size:xx-large;font-weight:bold;" for="connectionStringTestModal-ProgressBar">Testing your connection string...</label>
                <progress style="height:2.5rem;width:25rem;" id="connectionStringTestModal-ProgressBar"></progress>
            </div>
        </div>
        <!-- Copy Complete Modal Window -->
        <div id="AlertModal" class="AlertModal">
            <div class="AlertModal-Container">
                <div id="AlertModal-TitleBar" class="AlertModal-TitleBar">
                    <p>Web Application Copy Complete</p>
                </div>
                <div class="AlertModal-Body">
                    <div id="AlertModal-Text" class="AlertModal-Text">
                        <p id="AlertModal-Text-Paragraph"></p>
                    </div>
                    <div id="AlertModal-Button" class="AlertModal-Button">
                        <button onclick="closeAlertModal()">OK</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- Loader Modal Window -->
        <div class="loader"></div>
        <!-- Successful save Modal -->
        <div id="ProcessingWebConfigValuesProgress" class="ProcessingWebConfigValuesProgress">
        <div id="ProcessingWebConfigValuesProgress-Content" class="ProcessingWebConfigValuesProgress-Content">
                <div id="ProcessingWebConfigValuesProgress-Section" class="ProcessingWebConfigValuesProgress-Section">
                    <label id="ProcessingWebConfig-Label" class="ProcessingWebConfigValuesProgress-Label" for="ProcessingWebConfigValuesProgress-Bar"></label>
                    <progress class="ProcessingWebConfigValuesProgress-Bar" id="ProcessingWebConfigValuesProgress-Bar"></progress>
                </div>
            </div>
        </div>
        <!-- Configuration Errors Modal Window -->
        <div id="SaveErrors-Modal" class="sectionWithTitleBar">
        <div id="SaveErrors-Content" class="SaveErrors-Content">
                <div class="titleBar" name="Title-Bar">
                    <h2>Errors</h2>
                </div>
                <div class="coreSection">
                    <div id="SaveErrors" class="SaveErrors">
                        <div id="SaveErrors-Section" class="SaveErrors-Section">
                            <div class="SaveErrors-ErrorText">
                                <!--This is where the error text will be.-->
                                <p>Please review the errors below. You and click on one of them and then click View to jump to that location.</p>
                            </div>
                            <div class="SaveErrors-ErrorTable">
                                <!--This is where the "table" of the different errors will go.-->
                                <table id="SaveErrorsTable">
                                    <thead>
                                        <tr>
                                            <th>Error #</th>
                                            <th>Section</th>
                                            <th>Title</th>
                                            <th>Reason</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                            <div class="SaveErrors-Footer">
                                <div class="SaveErrors-ButtonContainer">
                                    <button id="SaveErrors-ViewButton" class="SaveErrors-ViewButton" onclick="viewButtonClicked()" disabled>View</button>
                                    <button id="SaveErrors-CancelButton" class="SaveErrors-CancelButton" onclick="closeErrorModal()">Cancel</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Unknown configuration keys modal -->
        <div id="UnknownConfigurationKeys-Modal" class="sectionWithTitleBar">
            <div class="UnknownConfigurationKeys-Content">
                <div class="titleBar" name="Title-Bar">
                    <h2>Unknown Configuration Keys</h2>
                    <p id="UnknownConfigurationKeys-CloseModal" onclick="closeUnknownConfigurationKeysModal()">X</p>
                </div>
                <div class="coreSection">
                    <div id="UnknownConfigurationKeys" class="UnknownConfigurationKeys">
                        <div id="UnknownConfigurationKeysElements-Section" class="UnknownConfigurationKeys-Section">
                            <div class="UnknownConfigurationKeys-Text">
                                <p>The following web.config key elements are unknown to be parsed. These need to be reported so they can be added.</p>
                            </div>
                            <div class="UnknownConfigurationKeys-ElementsTable">
                                <table id="UnknownConfigurationKeysElementsTable">
                                    <thead>
                                        <tr>
                                            <th>Section</th>
                                            <th>Path Name</th>
                                            <th>Attribute Name</th>
                                            <th>Type</th>
                                            <th>HTML ID Name</th>
                                            <th>Value</th>
                                            <th>Version</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                            <div class="UnknownConfigurationKeys-Footer">
                                <div class="UnknownConfigurationKeys-ButtonContainer">
                                    <button id="UnknownConfigurationKeys-CopyToClipboardButton" class="UnknownConfigurationKeys-CopyToClipboardButton" onclick="copyToClipboardPreparation('Elements')">Copy To Clipboard</button>
                                    <button id="UnknownConfigurationKeys-SendInEmailButton" class="UnknownConfigurationKeys-SendInEmailButton" onclick="">Send In Email</button>
                                </div>
                            </div>
                        </div>
                        <hr />
                        <div id="UnknownConfigurationKeysAttributes-Section" class="UnknownConfigurationKeys-Section">
                            <div class="UnknownConfigurationKeys-Text">
                                <p>The following web.config key attributes are unknown to be parsed. These need to be reported so they can be added.</p>
                            </div>
                            <div class="UnknownConfigurationKeys-AttributesTable">
                                <table id="UnknownConfigurationKeysAttributesTable">
                                    <thead>
                                        <tr>
                                            <th>Section</th>
                                            <th>Path Name</th>
                                            <th>Attribute Name</th>
                                            <th>Type</th>
                                            <th>HTML ID Name</th>
                                            <th>Value</th>
                                            <th>Version</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                            <div class="UnknownConfigurationKeys-Footer">
                                <div class="UnknownConfigurationKeys-ButtonContainer">
                                    <button id="UnknownConfigurationKeys-CopyToClipboardButton" class="UnknownConfigurationKeys-CopyToClipboardButton" onclick="copyToClipboardPreparation('Attributes')">Copy To Clipboard</button>
                                    <button id="UnknownConfigurationKeys-SendInEmailButton" class="UnknownConfigurationKeys-SendInEmailButton" onclick="">Send In Email</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Menu Bar -->
        <header>
            <div id="h1-container" class="h1-container">
                <div style="display:flex;justify-content:space-between;padding-left: 0.5%;">
                    <a style="color:inherit;text-decoration: none;" href="/"><h1 b-8gj57b4bak="">COMACON</h1></a>
                    <div style="margin: auto 5px;padding-left: 0.5%;">
                        <input type="checkbox" class="checkbox" id="checkbox" onclick="toggleDarkLightMode(this)">
                        <label for="checkbox" class="checkbox-label">
                            <i class="fas fa-moon">
                                <svg width="18" height="18" fill="none" stroke="#f39c12" stroke-linecap="round" stroke-linejoin="round" stroke-width="3.5" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M21 12.79A9 9 0 1 1 11.21 3 7 7 0 0 0 21 12.79Z"></path>
                                </svg>
                            </i>
                            <i class="fas fa-sun">
                                <svg width="18" height="18" fill="none" stroke="#f39c12" stroke-linecap="round" stroke-linejoin="round" stroke-width="3.5" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M12 7a5 5 0 1 0 0 10 5 5 0 1 0 0-10z"></path>
                                    <path d="M12 1v2"></path>
                                    <path d="M12 21v2"></path>
                                    <path d="m4.22 4.22 1.42 1.42"></path>
                                    <path d="m18.36 18.36 1.42 1.42"></path>
                                    <path d="M1 12h2"></path>
                                    <path d="M21 12h2"></path>
                                    <path d="m4.22 19.78 1.42-1.42"></path>
                                    <path d="m18.36 5.64 1.42-1.42"></path>
                                </svg>
                            </i>
                            <span class="ball"></span>
                        </label>
                    </div>
                </div>
                <p id="LoadedApplicationType" style="margin: 0;padding-left: 0.5%;padding-bottom: 0.5%;"></p>
                <p id="LoadedApplicationName" style="margin: 0;padding-left: 0.5%;padding-bottom: 0.5%;"></p>
                <div id="core-action-buttons-div" class="core-action-buttons-div">
                    <div style="display: flex;">
                        <button id="Open-Web-Application-Core-Action-Button" class="core-action-buttons" type="button" onclick="openModal()"><u>Open Web Application</u></button>
                        <button id="Copy-Web-Application-Core-Action-Button" class="core-action-buttons Web-Application-Button" type="button" onclick="openCopyWebApplicationModal()"><u>Copy Web Application</u></button>
                        <button id="Save-Web-Application-Core-Action-Button" class="core-action-buttons Web-Application-Button" type="button" onclick="configOkToSave()"><u>Save Web Application</u></button>
                    </div>
                    <div id="container">
                        <a id="Unknown-Configuration-Keys-Link" href="javascript:openUnknownConfigurationKeysModal()" style="text-decoration: none;">Unknown Configuration Keys</a>
                        <button onclick="showErrorModal()">
                            <svg width="28" height="28" fill="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                                <path d="M20.066 15.86a11.443 11.443 0 0 1-.575-3.57V9.291a7.673 7.673 0 0 0-1.902-5.169 7.707 7.707 0 0 0-4.863-2.583c-1.041-.108-2.1 0-3.105.34A7.142 7.142 0 0 0 6.93 3.46a7.384 7.384 0 0 0-1.812 2.513 7.6 7.6 0 0 0-.647 3.033v3.3a11.414 11.414 0 0 1-.574 3.555L3 18.514l.719.987h5.257c0 .79.322 1.56.879 2.118a3.02 3.02 0 0 0 2.118.88c.789 0 1.56-.323 2.116-.88a3.02 3.02 0 0 0 .881-2.118h5.257l.718-.987-.88-2.655Zm-7.035 4.72a1.496 1.496 0 0 1-1.058.448 1.495 1.495 0 0 1-1.059-.45 1.495 1.495 0 0 1-.45-1.057h2.998a1.407 1.407 0 0 1-.43 1.059h-.002Zm-8.273-2.565.556-1.671c.434-1.302.659-2.665.665-4.037V9.006c0-.845.18-1.67.52-2.424a5.78 5.78 0 0 1 1.454-2.01 5.831 5.831 0 0 1 2.154-1.256 5.81 5.81 0 0 1 2.475-.27 6.22 6.22 0 0 1 3.895 2.1 6.2 6.2 0 0 1 1.506 4.164v3.015c0 1.364.216 2.727.665 4.037l.556 1.67H4.76v-.019l-.001.002Z"></path>
