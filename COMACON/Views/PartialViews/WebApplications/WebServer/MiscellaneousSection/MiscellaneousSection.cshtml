    <div class="row">
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Keyword Typeahead
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="KeywordTypeahead-KeywordIDNumber">Keyword ID#: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span id="KeywordTypeahead-KeywordIDNumber-Tooltip" class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input type="number" name="KeywordTypeahead-KeywordIDNumber" id="KeywordTypeahead-KeywordIDNumber" oninput="keywordDropDownTypeAheadFieldUpdated(this)" disabled>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="KeywordTypeahead-TypeaheadCharacterCount">Typeahead character count: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span id="KeywordTypeahead-TypeaheadCharacterCount-Tooltip" class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input type="number" name="KeywordTypeahead-TypeaheadCharacterCount" id="KeywordTypeahead-TypeaheadCharacterCount" oninput="keywordDropDownTypeAheadFieldUpdated(this)" disabled>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;">
                        <div style="display: flex; width: 40%; justify-content: space-around;">
                            <button id="KeywordTypeahead-AddButton" onclick="addKeywordDropDownTypeAhead()">Add</button>
                            <button id="KeywordTypeahead-DeleteButton" onclick="deleteKeywordDropDownTypeAhead()" disabled>Delete</button>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                        <select class="" name="KeywordTypeahead-SelectList" id="KeywordTypeahead-SelectList" size="8" style="max-width:90%;min-width:50%" onchange="keywordTypeAheadMinimumCharactersChanged(this)">
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <div class="column">
