<div id="PublicCommentIntegrationsSection" class="tabcontent">
    <div style="display:flex; justify-content:space-between;">
        <div class="column" style="width:30%; padding:0;">
            <select id="PublicCommentIntegrations-SelectList" size="30" onchange="integrationSelected(this)" style="width: 100%;">
            </select>
            <div style="display: flex;justify-content: space-around;padding-top: 5px;">
                <button id="PublicCommentIntegrations-AddButton" onclick="addNewIntegration()" type="button">Add</button>
                <button id="PublicCommentIntegrations-CopyButton" onclick="copyIntegration()" type="button">Copy</button>
                <button id="PublicCommentIntegrations-DeleteButton" onclick="deleteIntegration()" type="button">Delete</button>
            </div>
        </div>
        <div class="column" style="width:65%;padding:0;">
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Integration-Name">Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span id="Integration-Name-Tooltip" class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <input type="text" name="Integration-Name" id="Integration-Name" oninput="integrationfieldUpdated(this)">
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Integration-URL">URL: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span id="Integration-URL-Tooltip" class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <input type="url" name="Integration-URL" id="Integration-URL" oninput="integrationfieldUpdated(this)">
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Integration-Token">Token: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span id="Integration-Token-Tooltip" class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <input type="text" name="Integration-Token" id="Integration-Token" oninput="integrationfieldUpdated(this)">
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Availability-From-Meeting-Start">Availability From Meeting Start: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span id="Availability-From-Meeting-Start-Tooltip" class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <input type="number" name="Availability-From-Meeting-Start" id="Availability-From-Meeting-Start" oninput="integrationfieldUpdated(this)">
                </div>
            </div>
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Agenda Unity Form Fields
                </div>
                <div class="coreSection">
                    <div class="row">
                        <div class="column">
                            <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                                <select class="" name="Form-Field-Select-List" id="Form-Field-Select-List" size="8" style="max-width:90%;min-width:50%" onchange="unityFormFieldSelected(this)">
                                </select>
                            </div>
                            <div style="display: flex; justify-content: center;">
                                <div style="display: flex; width: 40%; justify-content: space-around;">
                                    <button id="FormFieldSelectList-AddButton" onclick="addNewUnityFormField()" disabled>Add</button>
                                    <button id="FormFieldSelectList-DeleteButton" onclick="deleteUnityFormField()" disabled>Delete</button>
                                </div>
                            </div>
                        </div>
                        <div class="column">
                            <div class="settingElement">
                                <div class="labelElement">
                                    <label for="Form-Field-Name">Field Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                    <span id="Form-Field-Name-Tooltip" class="tooltiptext">This is the help text.</span>
                                </div>
                                <div class="inputElement">
                                    <input type="text" name="Form-Field-Name" id="Form-Field-Name" oninput="integrationfieldUpdated(this)">
                                </div>
                            </div>
                            <div class="settingElement">
                                <div class="labelElement">
                                    <label for="Form-Field-ID">Form Field ID: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                    <span id="Form-Field-ID-Tooltip" class="tooltiptext">This is the help text.</span>
                                </div>
                                <div class="inputElement">
                                    <input type="text" name="Form-Field-ID" id="Form-Field-ID" oninput="integrationfieldUpdated(this)">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Meeting Types
                </div>
                <div class="coreSection">
                    <div class="row">
                        <div class="column">
                            <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                                <select class="" name="Meeting-Type-Name-Select-List" id="Meeting-Type-Name-Select-List" size="8" style="max-width:90%;min-width:50%" onchange="meetingtypeSelected(this)">
                                </select>
                            </div>
                            <div style="display: flex; justify-content: center;">
                                <div style="display: flex; width: 40%; justify-content: space-around;">
                                    <button id="Meeting-Type-Name-Select-List-AddButton" onclick="addNewMeetingType()" disabled>Add</button>
                                    <button id="Meeting-Type-Name-Select-List-DeleteButton" onclick="deleteMeetingType()" disabled>Delete</button>
                                </div>
                            </div>
                        </div>
                        <div class="column">
                            <div class="settingElement">
                                <div class="labelElement">
                                    <label for="Meeting-Type-Name">Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                    <span id="Meeting-Type-Name-Tooltip" class="tooltiptext">This is the help text.</span>
                                </div>
                                <div class="inputElement">
                                    <input type="text" name="Meeting-Type-Name" id="Meeting-Type-Name" oninput="integrationfieldUpdated(this)">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>