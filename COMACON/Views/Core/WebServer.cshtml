@{
    ViewData["Title"] = "Web Server";
}

<div id="ActiveXSection" class="tabcontent">
    <div class="row">
        <p class="BlockDescription">The following settings are only used when the Web Client is in ActiveX mode.</p>
    </div>
    <div class="row">
        <div class="column">
            <div class="settingElement" id="Autozoom-Thumbnail-Configuration-SettingElement">
                <div class="labelElement">
                    <label for="Autozoom-Thumbnail-Configuration">Autozoom Thumbnail Configuration: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">
                        <span>
                            If "True" - The thumbnail is always auto zoomed regardless of local setting.
                        </span>
                        <br>
                        <span>
                            If "False" -  The thumbnail is not auto zoomed regardless of local setting.
                        </span>
                        <br>
                        <span>
                            If "Local" - The thumbnail auto zoom depends on local setting.
                        </span>
                    </span>
                </div>
                <div class="inputElement">
                    <select name="Autozoom-Thumbnail-Configuration" id="Autozoom-Thumbnail-Configuration">
                        <option value="true">true</option>
                        <option value="false">false</option>
                        <option value="local" selected>local</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="column">
        </div>
    </div>
</div>
<div id="ADFSSettingsSection" class="tabcontent">
    <div class="row">
        <p class="BlockDescription">The following keys are the settings for enabling ADFS. In order to properly configure ADFS, you must run the "Configure For ADFS" menu item in the Tools menu.</p>
    </div>
    <div class="row">
        <div class="column">
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Authentication-Mode">Authentication Mode: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <select name="Authentication-Mode" id="Authentication-Mode">
                        <option value="Windows">Windows</option>
                        <option value="None">None</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="column">
            <div class="settingElement">
                <div class="labelElement">
                    <span>ADFS Enabled: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="ADFS-Enabled" class="switch">
                        <input onclick="adfsEnabledCheckbox(this)" id="ADFS-Enabled" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Request-Validation-Mode">Request Validation Mode: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">The certificate identified by the thumbprint in the trustedIssuers element is the token-signing certificate found on the ADFS server.</span>
                </div>
                <div class="inputElement">
                    <input type="text" name="Request-Validation-Mode" id="Request-Validation-Mode" disabled>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <span>Synchronize User Attributes: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Synchronize-User-Attributes" class="switch">
                        <input name="Synchronize-User-Attributes" id="Synchronize-User-Attributes" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <span>Authentication Only: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Authentication-Only" class="switch">
                        <input name="Authentication-Only" id="Authentication-Only" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <span>Cookie Handler - Require SSL: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Cookie-Handler-Require-SSL" class="switch">
                        <input name="Cookie-Handler-Require-SSL" id="Cookie-Handler-Require-SSL" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Audience URI's
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Audience-URI">Audience URI: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">The Audience URI identifies the relying party (RP - this application).  This much exactly (case, trailing slash, etc.) match the Relying Party Trust Identifier on the ADFS sever.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-Fields" type="url" name="Audience-URI" id="Audience-URI" oninput="updateAudienceUri(this)" disabled>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;">
                        <div style="display: flex; width: 40%; justify-content: space-around;">
                            <button id="AudienceURI-AddButton" onclick="addAudienceUri()">Add</button>
                            <button id="AudienceURI-DeleteButton" onclick="deleteAudienceUri()" disabled>Delete</button>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                        <select class="" name="AudienceURI-SelectList" id="AudienceURI-SelectList" size="8" style="max-width:90%;min-width:50%" onchange="audienceUriChanged(this)">
                        </select>
                    </div>
                </div>
            </div>
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Trusted Issuers
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Trusted-Issuer-Thumbprint">Trusted Issuer Thumbprint: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">The certificate identified by the thumbprint in the trustedIssuers element is the token-signing certificate found on the ADFS server.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-Fields" type="text" name="Trusted-Issuer-Thumbprint" id="Trusted-Issuer-Thumbprint" oninput="updateTrustedIssuerThumbprint(this)" disabled>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Trusted-Issuer-Name">Trusted Issuer Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-Fields" type="text" name="Trusted-Issuer-Name" id="Trusted-Issuer-Name" oninput="updateTrustedIssuerName(this)" disabled>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;">
                        <div style="display: flex; width: 40%; justify-content: space-around;">
                            <button id="TrustedIssuer-AddButton" onclick="addTrustedIssuer()">Add</button>
                            <button id="TrustedIssuer-DeleteButton" onclick="deleteTrustedIssuer()" disabled>Delete</button>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                        <select class="" name="TrustedIssuers-SelectList" id="TrustedIssuers-SelectList" size="8" style="max-width:90%;min-width:50%" onchange="trustedIssuerChanged(this)">
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    system.identitymodel.services
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="wsFederation-Issuer">wsFederation Issuer: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="wsFederation-Issuer" id="wsFederation-Issuer">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="wsFederation-Realm">wsFederation Realm: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="wsFederation-Realm" id="wsFederation-Realm">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-X509-Find-Type">Certificate X509 Find Type: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="Certificate-X509-Find-Type" id="Certificate-X509-Find-Type">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-Find-Value">Certificate Find Value: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="Certificate-Find-Value" id="Certificate-Find-Value">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-Store-Location">Certificate Store Location: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="Certificate-Store-Location" id="Certificate-Store-Location">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-Store-Name">Certificate Store Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <select class="adfs-system-identityModel-services-Fields" name="Certificate-Store-Name" id="Certificate-Store-Name">
                                <option value="AddressBook">AddressBook</option>
                                <option value="AuthRoot">AuthRoot</option>
                                <option value="CertificateAuthority">CertificateAuthority</option>
                                <option value="Disallowed">Disallowed</option>
                                <option value="My" selected>My</option>
                                <option value="Root">Root</option>
                                <option value="TrustedPeople">TrustedPeople</option>
                                <option value="TrustedPublisher">TrustedPublisher</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="CustomValidationSection" class="tabcontent">
    <div class="row">
        <p style="border: 1px solid black;padding: 5px;">The Custom Validation section defines custom validation to be performed on specific keyword types. Custom validation can be defined to occur across the entire application and for individual pages. If a keyword type is defined at both the application and page level, the page level definition will take precedence on that page only. Page and script file locations are all relative to the virtual root and are case-insensitive. However, validator names are case-sensitive.</p>
    </div>
    <div class="sectionWithTitleBar">
        <div class="titleBar" name="Title-Bar">
            Application Level
        </div>
        <div class="coreSection">
            <div class="row">
                <div class="settingElement" style="justify-content: left;">
                    <div class="labelElement">
                        <label for="Application-Script-Location">Script Location: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                        <span class="tooltiptext">This is the help text.</span>
                    </div>
                    <div class="inputElement">
                        <input type="text" name="Application-Script-Location" id="Application-Script-Location" class="longTextInputBox">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="sectionWithTitleBar">
                    <div class="titleBar" name="Title-Bar">
                        Keywords
                    </div>
                    <div class="coreSection">
                        <div class="row">
                            <div class="column" style="width: 30%;">
                                <p>Keywords</p>
                                <select id="CustomValidation-ApplicationLevel-SelectList" style="width:100%;" size="15" onchange="customValidationSelectionChanged('Application')">
                                </select>
                                <div name="actionbuttons" class="CustomValidationApplicationListKeywordsButtons">
                                    <button id="Custom-Validation-Application-Add-Button" onclick="addCustomValidationKeywordConfiguration('Application')">Add</button>
                                    <button id="Custom-Validation-Application-Delete-Button" onclick="deleteCustomValidationKeywordConfiguration('Application')">Delete</button>
                                </div>
                            </div>
                            <div class="column">
                                <div class="settingElement" style="margin-top: 15%;">
                                    <div class="labelElement">
                                        <label for="Application-Keyword-ID">Keyword ID: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                        <span class="tooltiptext">This is the help text.</span>
                                    </div>
                                    <div class="inputElement">
                                        <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Application-Keyword-ID-RequiredSvg" class="requiredAlertSvg">
                                            <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                                        </svg>
                                        <input type="number" pattern="[0-9]*" inputmode="numeric" name="Application-Keyword-ID" id="Application-Keyword-ID" oninput="customValidationKeywordConfigurationFieldUpdated('Application')">
                                    </div>
                                </div>
                                <div class="settingElement">
                                    <div class="labelElement">
                                        <label for="Application-Keyword-Validator">Keyword Validator Function: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                        <span class="tooltiptext">This is the help text.</span>
                                    </div>
                                    <div class="inputElement">
                                        <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Application-Keyword-Validator-RequiredSvg" class="requiredAlertSvg">
                                            <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                                        </svg>
                                        <input type="text" name="Application-Keyword-Validator" id="Application-Keyword-Validator" oninput="customValidationKeywordConfigurationFieldUpdated('Application')">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="sectionWithTitleBar">
        <div class="titleBar" name="Title-Bar">
            Page Level
        </div>
        <div class="coreSection">
            <div class="row" style="margin-top:1rem;">
                <div class="column" style="width: 30%;">
                    <p style="margin: 0%;">Pages</p>
                    <select id="CustomValidation-PageLevel-SelectList" style="width:100%;" size="25" onchange="customValidationSelectionChanged('Page')">
                    </select>
                </div>
                <div class="column" style="width: 70%;">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Page-Location">Page Location: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input type="text" name="Page-Location" id="Page-Location" class="longTextInputBox" disabled>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Page-Script-Location">Page Script Location: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input type="text" name="Page-Script-Location" id="Page-Script-Location" class="longTextInputBox">
                        </div>
                    </div>
                    <div class="sectionWithTitleBar">
                        <div class="titleBar" name="Title-Bar">
                            Keywords
                        </div>
                        <div class="coreSection">
                            <div class="row">
                                <div class="column">
                                    <p>Keywords</p>
                                    <select id="CustomValidation-PageLevel-Keywords-SelectList" size="25" style="width:100%;" onchange="customValidationPageLevelKeywordSelectionChanged()">
                                    </select>
                                    <div name="actionbuttons" class="CustomValidationPagesListKeywordsButtons">
                                        <button id="customValidationPagesAddKeywordButton" onclick="addCustomValidationKeywordConfiguration('Page')" type="button">Add</button>
                                        <button id="customValidationPagesDeleteKeywordButton" onclick="deleteCustomValidationKeywordConfiguration('Page')" type="button">Delete</button>
                                    </div>
                                </div>
                                <div class="column">
                                    <div class="settingElement" style="margin-top: 5rem;">
                                        <div class="labelElement">
                                            <label for="Page-Keyword-ID">Keyword ID: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                            <span class="tooltiptext">This is the help text.</span>
                                        </div>
                                        <div class="inputElement">
                                            <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Page-Keyword-ID-RequiredSvg" class="requiredAlertSvg">
                                                <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                                            </svg>
                                            <input type="number" pattern="[0-9]*" inputmode="numeric" name="Page-Keyword-ID" id="Page-Keyword-ID" oninput="customValidationKeywordConfigurationFieldUpdated('Page')">
                                        </div>
                                    </div>
                                    <div class="settingElement">
                                        <div class="labelElement">
                                            <label for="Page-Keyword-Validator">Keyword Validator Function: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                            <span class="tooltiptext">This is the help text.</span>
                                        </div>
                                        <div class="inputElement">
                                            <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Page-Keyword-Validator-RequiredSvg" class="requiredAlertSvg">
                                                <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                                            </svg>
                                            <input type="text" name="Page-Keyword-Validator" id="Page-Keyword-Validator" oninput="customValidationKeywordConfigurationFieldUpdated('Page')">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="DashboardViewerSection" class="tabcontent">
    <div class="row">
        <div class="column">
            <div class="settingElement" id="Use-Theme-SettingElement">
                <div class="labelElement">
                    <label for="Use-Theme">Use Theme: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <select name="Use-Theme" id="Use-Theme">
                        <option value="light" selected>light</option>
                        <option value="dark">dark</option>
                    </select>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Reporting-Page-Limit">Reporting Page Limit: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <input type="number" pattern="[0-9]*" inputmode="numeric" name="Reporting-Page-Limit" id="Reporting-Page-Limit" value="50">
                </div>
            </div>
            <div class="settingElement" id="Dashboard-Cache-Timeout-SettingElement">
                <div class="labelElement">
                    <label for="Dashboard-Cache-Timeout">Dashboard Cache Timeout: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <input type="number" name="Dashboard-Cache-Timeout" id="Dashboard-Cache-Timeout">
                </div>
            </div>
        </div>
        <div class="column">
            <div class="settingElement">
                <div class="labelElement">
                    <span>Enable Export Dashboard: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Enable-Export-Dashboard" class="switch">
                        <input id="Enable-Export-Dashboard" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
