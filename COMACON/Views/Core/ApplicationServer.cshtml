@{
    ViewData["Title"] = "Application Server";
}

<div id="ADFSSection" class="tabcontent">
    <div class="row">
        <p class="BlockDescription">The following keys are the settings for enabling ADFS. In order to properly configure ADFS, you must run the "Configure For ADFS" menu item in the Tools menu.</p>
    </div>
    <div class="row">
        <div class="column">
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Authentication-Mode">Authentication Mode: <img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark"></label>
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <select name="Authentication-Mode" id="Authentication-Mode">
                        <option value="Windows">Windows</option>
                        <option value="None">None</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="column">
            <div class="settingElement">
                <div class="labelElement">
                    <span>ADFS Enabled: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="ADFS-Enabled" class="switch">
                        <input onclick="adfsEnabledCheckbox(this)" id="ADFS-Enabled" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <label for="Request-Validation-Mode">Request Validation Mode: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">The certificate identified by the thumbprint in the trustedIssuers element is the token-signing certificate found on the ADFS server.</span>
                </div>
                <div class="inputElement">
                    <input type="text" name="Request-Validation-Mode" id="Request-Validation-Mode" disabled>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <span>Synchronize User Attributes: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Synchronize-User-Attributes" class="switch">
                        <input name="Synchronize-User-Attributes" id="Synchronize-User-Attributes" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <span>Authentication Only: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Authentication-Only" class="switch">
                        <input name="Authentication-Only" id="Authentication-Only" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
            <div class="settingElement">
                <div class="labelElement">
                    <span>Cookie Handler - Require SSL: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Cookie-Handler-Require-SSL" class="switch">
                        <input name="Cookie-Handler-Require-SSL" id="Cookie-Handler-Require-SSL" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Audience URI's
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Audience-URI">Audience URI: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">The Audience URI identifies the relying party (RP - this application).  This much exactly (case, trailing slash, etc.) match the Relying Party Trust Identifier on the ADFS sever.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-Fields" type="url" name="Audience-URI" id="Audience-URI" oninput="updateAudienceUri(this)" disabled>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;">
                        <div style="display: flex; width: 40%; justify-content: space-around;">
                            <button id="AudienceURI-AddButton" onclick="addAudienceUri()">Add</button>
                            <button id="AudienceURI-DeleteButton" onclick="deleteAudienceUri()" disabled>Delete</button>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                        <select class="" name="AudienceURI-SelectList" id="AudienceURI-SelectList" size="8" style="max-width:90%;min-width:50%" onchange="audienceUriChanged(this)">
                        </select>
                    </div>
                </div>
            </div>
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Trusted Issuers
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Trusted-Issuer-Thumbprint">Trusted Issuer Thumbprint: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">The certificate identified by the thumbprint in the trustedIssuers element is the token-signing certificate found on the ADFS server.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-Fields" type="text" name="Trusted-Issuer-Thumbprint" id="Trusted-Issuer-Thumbprint" oninput="updateTrustedIssuerThumbprint(this)" disabled>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Trusted-Issuer-Name">Trusted Issuer Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-Fields" type="text" name="Trusted-Issuer-Name" id="Trusted-Issuer-Name" oninput="updateTrustedIssuerName(this)" disabled>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;">
                        <div style="display: flex; width: 40%; justify-content: space-around;">
                            <button id="TrustedIssuer-AddButton" onclick="addTrustedIssuer()">Add</button>
                            <button id="TrustedIssuer-DeleteButton" onclick="deleteTrustedIssuer()" disabled>Delete</button>
                        </div>
                    </div>
                    <div style="display: flex; justify-content: center;margin: 1rem 0 1rem 0;">
                        <select class="" name="TrustedIssuers-SelectList" id="TrustedIssuers-SelectList" size="8" style="max-width:90%;min-width:50%" onchange="trustedIssuerChanged(this)">
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    system.identitymodel.services
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="wsFederation-Issuer">wsFederation Issuer: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="wsFederation-Issuer" id="wsFederation-Issuer">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="wsFederation-Realm">wsFederation Realm: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="wsFederation-Realm" id="wsFederation-Realm">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-X509-Find-Type">Certificate X509 Find Type: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="Certificate-X509-Find-Type" id="Certificate-X509-Find-Type">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-Find-Value">Certificate Find Value: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="Certificate-Find-Value" id="Certificate-Find-Value">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-Store-Location">Certificate Store Location: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input class="adfs-system-identityModel-services-Fields" type="text" name="Certificate-Store-Location" id="Certificate-Store-Location">
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Certificate-Store-Name">Certificate Store Name: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <select class="adfs-system-identityModel-services-Fields" name="Certificate-Store-Name" id="Certificate-Store-Name">
                                <option value="AddressBook">AddressBook</option>
                                <option value="AuthRoot">AuthRoot</option>
                                <option value="CertificateAuthority">CertificateAuthority</option>
                                <option value="Disallowed">Disallowed</option>
                                <option value="My" selected>My</option>
                                <option value="Root">Root</option>
                                <option value="TrustedPeople">TrustedPeople</option>
                                <option value="TrustedPublisher">TrustedPublisher</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="ApplicationServerSection" class="tabcontent">
    <div class="row">
        <p class="BlockDescription">The following settings are the basic settings for the App Server.</p>
    </div>
    <div class="row">
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Core
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Data-Source">Data Source: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <select name="Data-Source" id="Data-Source">
                            </select>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Server-Designation">Server Designation: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">The name for this application server.<br>This is useful to see what server a user is conencted to in a load-balanced environment.</span>
                        </div>
                        <div class="inputElement">
                            <input type="text" name="Server-Designation" id="Server-Designation">
                        </div>
                    </div>
                </div>
            </div>
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Session
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <span>Session Enable Timeout: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">Controls the timeout behavior for unused sessions. If enableTimeout is set to true the AppServer will timeout unused sessions and disconnect them. The timeout period is based on the ASP.NET session timeout found in the system.web\session section with a minimum of 10 minutes.</span>
                        </div>
                        <div class="inputElement">
                            <label for="Session-Enable-Timeout" class="switch">
                                <input id="Session-Enable-Timeout" type="checkbox" onchange="sessionTimeoutChecked(this,'Session-Timeout-Length')">
                                <span class="slider round"></span>
                            </label>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <label for="Session-Timeout-Length">Session Timeout Length: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is the help text.</span>
                        </div>
                        <div class="inputElement">
                            <input type="number" name="Session-Timeout-Length" id="Session-Timeout-Length">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="column">
            <div class="sectionWithTitleBar">
                <div class="titleBar" name="Title-Bar">
                    Security & Authentication
                </div>
                <div class="coreSection">
                    <div class="settingElement">
                        <div class="labelElement">
                            <span>Allow Insecure Connections: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This setting controls if the server will accept only https connections or not. If you set this to false all incoming requests have to be made over https. If you set this to true then both http and https connections are accepted. The recommended setting is false.</span>
                        </div>
                        <div class="inputElement">
                            <label for="Allow-Insecure-Connections" class="switch">
                                <input id="Allow-Insecure-Connections" type="checkbox">
                                <span class="slider round"></span>
                            </label>
                        </div>
                    </div>
                    <div class="settingElement">
                        <div class="labelElement">
                            <span>Use OS Authentication: </span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                            <span class="tooltiptext">This is used to determine if the connection to the database should be made with OS authentication or SQL Authentication. If enabled, the OS/domain user that is configured for this application needs to have to have the same database permissions as the "hsi" user would have.</span>
                        </div>
                        <div class="inputElement">
                            <label for="Use-OS-Authentication" class="switch">
                                <input id="Use-OS-Authentication" type="checkbox">
                                <span class="slider round"></span>
                            </label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="ExtraRowSection">
        <div class="sectionWithTitleBar">
            <div class="titleBar" name="Title-Bar">
                Miscellaneous
            </div>
            <div class="coreSection">
                <div class="row">
                    <div class="column">
                        <div class="settingElement">
                            <div class="labelElement">
                                <label for="Http-Runtime-Maximum-Request-Length">HTTP Runtime Maximum Request Length: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">This is the help text.</span>
                            </div>
                            <div class="inputElement">
                                <input type="number" name="Http-Runtime-Maximum-Request-Length" id="Http-Runtime-Maximum-Request-Length">
                            </div>
                        </div>
                        <div class="settingElement">
                            <div class="labelElement">
                                <label for="Http-Runtime-Execution-Timeout">HTTP Runtime Execution Timeout: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">This is the help text.</span>
                            </div>
                            <div class="inputElement">
                                <input type="number" name="Http-Runtime-Execution-Timeout" id="Http-Runtime-Execution-Timeout">
                            </div>
                        </div>
                    </div>
                    <div class="column">
                        <div class="settingElement">
                            <div class="labelElement">
                                <label for="Max-Allowed-Content-Length">Max Allowed Content Length: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">This is the help text.</span>
                            </div>
                            <div class="inputElement">
                                <input type="text" name="Max-Allowed-Content-Length" id="Max-Allowed-Content-Length">
                            </div>
                        </div>
                        <div class="settingElement">
                            <div class="labelElement">
                                <label for="Item-Cache-Timeout">Item Cache Timeout: </label><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                                <span class="tooltiptext">The number of seconds an item will exist within the item cache before it will be expired.</span>
                            </div>
                            <div class="inputElement">
                                <input type="text" name="Item-Cache-Timeout" id="Item-Cache-Timeout">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="ConnectionStringsSection" class="tabcontent">
    <div class="row">
        <div class="column" style="width: 25%;">
            <div class="settingElement">
                <div class="labelElement">
                    <span>Encrypt Connection Strings:</span><img class="tooltipimage" src="~/images/blue question mark icon - extra small.png" alt="Blue Tooltip Question Mark">
                    <span class="tooltiptext">This is the help text.</span>
                </div>
                <div class="inputElement">
                    <label for="Encrypt-Connection-Strings" class="switch">
                        <input id="Encrypt-Connection-Strings" type="checkbox">
                        <span class="slider round"></span>
                    </label>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="column" style="min-width: 25%;max-width:max-content;">
            <p style="margin: 0%;">Data Sources</p>
            <select id="ConnectionStrings-SelectList" size="10" onchange="connectionStringSelectedV3()" style="width: 100%;">
            </select>
            <div style="display: flex;justify-content: space-evenly;margin: 10px 0;">
                <button id="New-ConnectionString" onclick="newConnectionStringV3()" type="button" style="font-size:medium;">New</button>
                <button id="Copy-ConnectionString" onclick="copyConnectionStringV3()" type="button" style="font-size:medium;" disabled>Copy</button>
                <button id="Delete-ConnectionString" onclick="deleteConnectionStringV3()" type="button" style="font-size:medium;" disabled>Delete</button>
            </div>
        </div>
        <div class="column" style="width: 60%;display: flex;flex-direction:column;">
            <div name="CoreFields" style="display: flex;margin: 0.5rem 0;">
                <div style="padding: 0 0 0 10px;">
                    <div class="labelElement">
                        <label for="Data-Source-Name">Data Source Name</label>
                        <span class="tooltiptext">This is the help text.</span>
                    </div>
                    <div class="inputElement">
                        <input type="text" id="Data-Source-Name" name="Data-Source-Name" oninput="connectionStringTextInputFieldUpdatedV3(this)">
                        <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Data-Source-Name-RequiredSvg" class="coreConnectionStringRequiredAlertSvg">
                            <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                        </svg>
                    </div>
                    <div id="DuplicateConnectionString-Alert" style="color:red;font-weight:bolder;display:none;">Duplicate Name</div>
                </div>
                <div style="padding: 0 0 0 10px;">
                    <div class="labelElement">
                        <label for="Data-Provider">Data Provider</label>
                        <span class="tooltiptext">This is the help text.</span>
                    </div>
                    <div class="inputElement">
                        <select name="Data-Provider" id="Data-Provider" onchange="dataProviderV3(this)">
                            <option value="System.Data.SqlClient" selected>System.Data.SqlClient</option>
                            <option value="Oracle.ManagedDataAccess.Client">Oracle.ManagedDataAccess.Client</option>
                        </select>
                    </div>
                </div>
            </div>
            <div name="SqlServerFields" style="margin: 5px 0;">
                <div id="System.Data.SqlClient.Fields" style="display: flex;margin: 0.5rem 0;">
                    <div style="padding: 0 0 0 10px;">
                        <div class="labelElement">
                            <label for="Sql-Data-Source">Data Source</label>
                        </div>
                        <div class="inputElement">
                            <input type="text" id="Sql-Data-Source" name="Sql-Data-Source" oninput="connectionStringTextInputFieldUpdatedV3(this)">
                            <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Sql-Data-Source-RequiredSvg" class="sqlConnectionStringRequiredAlertSvg">
                                <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                            </svg>
                        </div>
                    </div>
                    <div style="padding: 0 0 0 10px;">
                        <div class="labelElement">
                            <label for="Sql-Database">Database</label>
                        </div>
                        <div class="inputElement">
                            <input type="text" id="Sql-Database" name="Sql-Database" oninput="connectionStringTextInputFieldUpdatedV3(this)">
                            <svg width="20" height="20" fill="#ff0000" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg" id="Sql-Database-RequiredSvg" class="sqlConnectionStringRequiredAlertSvg">
                                <path d="M16.325 2.183A.625.625 0 0 0 15.884 2H8.116a.625.625 0 0 0-.441.183L2.183 7.675A.625.625 0 0 0 2 8.116v7.768c0 .165.066.324.183.441l5.492 5.492a.625.625 0 0 0 .441.183h7.768a.625.625 0 0 0 .441-.183l5.492-5.492a.625.625 0 0 0 .183-.441V8.116a.625.625 0 0 0-.183-.441l-5.492-5.492ZM12 7a1.13 1.13 0 0 1 1.125 1.244l-.438 4.383a.69.69 0 0 1-1.374 0l-.438-4.383A1.131 1.131 0 0 1 12 7Zm.002 7.5a1.25 1.25 0 1 1 0 2.5 1.25 1.25 0 0 1 0-2.5Z"></path>
                            </svg>
                        </div>
                    </div>
                </div>
            </div>
            <div id="Oracle.ManagedDataAccess.Client.Fields" class="column" style="display: none; width: 100%;margin: 5px 0;">
                <div>
                    <input type="checkbox" name="Oracle-Fields-TNS-Connection-String" id="Oracle-Fields-TNS-Connection-String" checked onclick="tnsConnectionStringCheckCheckboxV3(this)">
                    <label for="Oracle-Fields-TNS-Connection-String">TNS Connection String</label>
                </div>
                <div style="display: flex;">
                    <div class="column" style="padding: 0 0 0 10px;">
                        <div style="padding: 5px 0">
                            <div class="labelElement">
                                <label for="Host">Host</label>
                            </div>
                            <div class="inputElement">
